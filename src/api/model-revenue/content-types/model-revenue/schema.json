{
  "kind": "collectionType",
  "collectionName": "model_revenues",
  "info": {
    "singularName": "model-revenue",
    "pluralName": "model-revenues",
    "displayName": "Model Revenue",
    "description": "Monthly revenue statistics per model from analytics"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "model_name": {
      "type": "string",
      "required": true
    },
    "platform_page": {
      "type": "string",
      "required": true
    },
    "month": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 11
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "messages_revenue": {
      "type": "decimal",
      "default": 0
    },
    "tips_revenue": {
      "type": "decimal",
      "default": 0
    },
    "subscriptions_revenue": {
      "type": "decimal",
      "default": 0
    },
    "total_revenue": {
      "type": "decimal",
      "default": 0
    },
    "model": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::model.model"
    }
  }
}
